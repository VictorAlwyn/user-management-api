{
  "name": "user-management-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "tsc && node --unhandled-rejections=strict ./dist/app.js",
    "debug": "export DEBUG=* && npm start",
    "test": "mocha -r ts-node/register 'src/test/**/*.test.ts' --unhandled-rejections=strict",
    "test-debug": "export DEBUG=* && npm test",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "migration:create": "sequelize migration:create",
    "model:create": "sequelize model:create",
    "seed": "NODE_PATH=. sequelize db:seed:all --seeders-path=./src/common/database/seeders",
    "seed:undo": "NODE_PATH=. sequelize db:seed:undo:all",
    "seed:create": "sequelize seed:create"
  },
  "dependencies": {
    "argon2": "^0.30.3",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "express-winston": "^4.2.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.1.0",
    "sequelize": "^6.28.0",
    "shortid": "^2.2.16",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/argon2": "^0.15.0",
    "@types/chai": "^4.3.4",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.16",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/shortid": "^0.0.29",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "sequelize-cli": "^6.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  }
}
